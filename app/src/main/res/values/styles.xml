<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="titleText">
        <item name="fontFamily">@font/mali_light</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="normalText">
        <item name="fontFamily">@font/mali_light</item>
        <item name="android:textSize">15sp</item>
        <item name="color">@color/white</item>
    </style>

    <style name="normalTextBold">
        <item name="fontFamily">@font/mali_light</item>
        <item name="android:textSize">15sp</item>
        <item name="color">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="buttonStyle1">
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/mali_light</item>
        <item name="android:width">75dp</item>
        <item name="height">50dp</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginStart">6dp</item>
        <item name="android:layout_marginEnd">3dp</item>
        <item name="android:textAllCaps">false</item>
    </style>


</resources>